{
  "expo": {
    "name": "iRaid",
    "slug": "iRaid",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    
    "android": {
      "splash": {
        "mdpi": "./assets/img/splash-mdpi.png",
        "hdpi": "./assets/img/splash-hdpi.png",
        "xhdpi": "./assets/img/splash-xhdpi.png",
        "xxhdpi": "./assets/img/splash-xxhdpi.png",
        "xxxhdpi": "./assets/img/splash-xxxhdpi.png",
        "resizeMode": "cover"
      }
    },
"ios": {
      "splash": {
        "image": "./assets/img/splash.png",
        "resizeMode": "cover"
      }
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    }
  }
}